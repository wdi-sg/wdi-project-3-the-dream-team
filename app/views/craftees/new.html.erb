<h1>Page for craftee to create craftee profile</h1>

<div class="container">
      <div class="row">
        <%= form_for @new_craftee do |f| %>
        <div class="row">
          <div class="input-field col s12">
            <input placeholder="Placeholder" id="name" type="text" class="validate">
            <%= f.label :name, id: 'name'%>
          </div>
        <%= f.submit 'Confirm', class: 'waves-effect waves-teal btn-flat', type:"submit"%>
        <% end %>
    </div>
</div>

 <pre>
   <%= @craftee.inspect %>
 </pre>
