<h1>Craftee's profile</h1>
<div class="container">
  <%= @craftee.sessions%>

  <%= @crafteeSessions = @craftee.sessions %>

  <%= month_calendar events: @crafteeSessions.each do |date,crafteeSessions | %>
    <%= date %>
    <% crafteeSessions.each do |crafteeSession| %>
    <div>
      <%= crafteeSession.name %>
    </div>
<% end %>
  <% end %>
    <!-- calender goes in here -->
    <ul>
      <% @craftee.sessions.each do |session| %>
      <li>session</li>
      <% end %>
    </ul>

    <ul>
      <% @craftee.fav_events.each do|fav_event| %>
        <li>fav_event.event</li>
      <% end %>
    </ul>

</div>
